{
  "name": "obsidian-community-plugin-browser",
  "version": "0.2.0",
  "description": "Browse and install community plugins from within Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "setup:dev": "node scripts/setup-dev.js",
    "install:dev": "node scripts/install-dev.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "plugin-browser"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.27.2",
    "eslint": "^9.18.0",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "obsidian": "latest",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
